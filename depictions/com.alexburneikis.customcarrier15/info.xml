<package>
	<id>com.supermamon.fullpackage</id>
	<name>Custom Carrier 15</name>
	<version>1.0.1</version>
	<compatibility>
		<firmware>
			<miniOS>15.0</miniOS>
			<maxOs>15.1.1</maxOs>
